{
    "name": "ttt",
    "version": "0.0.1",
    "description": "Tic Tac Toe",
    "main": "dist/index.js",
    "scripts": {
        "prebuild": "tslint -c tslint.json -p tsconfig.json --fix",
        "build": "tsc",
        "prestart": "npm run build",
        "start": "node .",
        "test": "nyc mocha --exit -r ts-node/register -r source-map-support/register --recursive ./**/*.test.ts"
    },
    "author": "Jon Yurek <jyurek@gmail.com>",
    "repository": "https://github.com/jyurek/ttt.git",
    "license": "UNLICENSED",
    "dependencies": {
        "dotenv": "^8.2.0",
        "express": "^4.17.3"
    },
    "devDependencies": {
        "@types/chai": "^4.2.4",
        "@types/chai-http": "^4.2.0",
        "@types/express": "^4.17.1",
        "@types/mocha": "^5.2.7",
        "@types/node": "^12.11.5",
        "chai": "^4.2.0",
        "chai-http": "^4.3.0",
        "mocha": "^6.2.2",
        "nyc": "^14.1.1",
        "prettier": "^1.18.2",
        "source-map-support": "^0.5.16",
        "ts-node": "^8.4.1",
        "tslint": "^5.20.0",
        "typescript": "^3.6.4"
    },
    "nyc": {
        "extension": [
            ".ts",
            ".tsx"
        ],
        "exclude": [
            "**/*.d.ts",
            "**/__tests__/**",
            "coverage"
        ],
        "reporter": [
            "html"
        ],
        "all": true
    }
}
